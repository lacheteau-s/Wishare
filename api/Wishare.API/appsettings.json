{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate":  "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
    },
  "AllowedHosts": "*"
}
